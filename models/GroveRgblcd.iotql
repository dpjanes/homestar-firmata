CREATE MODEL GroveRgblcd WITH
    schema:name = "grove-rgblcd"
ATTRIBUTE color WITH
    schema:name = "color",
    iot:purpose = iot-purpose:color,
    iot:type = iot:type.string,
    iot:format = iot:format.color
ATTRIBUTE text WITH
    schema:name = "text",
    iot:purpose = iot-purpose:message.text,
    iot:type = iot:type.string
;
